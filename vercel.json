{
    "version": 2,
    "builds": [
        {
            "src": "vachraj-strapi-fe/package.json",
            "use": "@vercel/next",
            "config": {
                "distDir": "vachraj-strapi-fe/.next"
            }
        },
        {
            "src": "vachraj-strapi-bk/package.json",
            "use": "@vercel/node",
            "config": {
                "distDir": "vachraj-strapi-bk/build"
            }
        }
    ],
    "routes": [
        {
            "src": "/strapi-admin/(.*)",
            "dest": "/vachraj-strapi-bk/public/admin/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/vachraj-strapi-bk/public/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/vachraj-strapi-fe/$1"
        }
    ]
}